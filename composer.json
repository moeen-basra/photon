{
  "name": "moeen-basra/photon",
  "description": "Laravel micro framework for SRP (Single Responsibility Principle)",
  "keywords": [
    "photon",
    "laravel",
    "SRP"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Moeen Basra",
      "email": "m.basra@live.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2",
    "orchestra/testbench": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "MoeenBasra\\Photon\\": "src/",
      "MoeenBasra\\Photon\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MoeenBasra\\Photon\\Providers\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
