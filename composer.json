{
  "name": "moeen-basra/photon",
  "description": "Laravel Extension for SRP (Single Responsibility Principle)",
  "keywords": [
    "photon",
    "laravel",
    "SRP"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Moeen Basra",
      "email": "m.basra@live.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.9",
    "orchestra/testbench": "^6.21"
  },
  "autoload": {
    "psr-4": {
      "Photon\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
